      "name": "Auth",
      "item": [
        {
          "name": "Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// Ensure the request succeeded\r",
                  "pm.test(\"Login response status is 200\", function () {\r",
                  "    pm.response.to.have.status(200);\r",
                  "});\r",
                  "\r",
                  "// Parse JSON response\r",
                  "let jsonData;\r",
                  "try {\r",
                  "    jsonData = pm.response.json();\r",
                  "} catch (e) {\r",
                  "    pm.test(\"Response body is valid JSON\", function () {\r",
                  "        throw new Error(\"Response is not valid JSON\");\r",
                  "    });\r",
                  "}\r",
                  "\r",
                  "// Read token from response.data.accessToken\r",
                  "const token = jsonData && jsonData.data && jsonData.data.accessToken;\r",
                  "\r",
                  "// Validate token presence and type\r",
                  "pm.test(\"Login response contains accessToken\", function () {\r",
                  "    pm.expect(token, \"accessToken should be present\")\r",
                  "        .to.be.a(\"string\")\r",
                  "        .that.is.not.empty;\r",
                  "});\r",
                  "\r",
                  "// Save token into active environment as ACCESS_TOKEN\r",
                  "if (token) {\r",
                  "    pm.environment.set(\"ACCESS_TOKEN\", token);\r",
                  "    pm.test(\"ACCESS_TOKEN environment variable set\", function () {\r",
                  "        pm.expect(pm.environment.get(\"ACCESS_TOKEN\")).to.eql(token);\r",
                  "    });\r",
                  "}"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            },
            {
              "listen": "prerequest",
              "script": {
                "exec": [
                  ""
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "",
                "value": "{{ACCESS_TOKEN}}",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\r\n    \"email\": \"test@example.com\",\r\n    \"password\": \"Test@123\"\r\n}",
              "options": {
